<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FastAFAppGP"
        tools:targetApi="31">
        <activity
            android:name=".ui.exchange.details.ExchangeDetails"
            android:exported="false" />
        <activity
            android:name=".ui.exchange.ExchangeActivity"
            android:exported="false" />
        <activity
            android:name=".ui.order.orderprocess.DeliveryActivity"
            android:exported="false" />
        <activity
            android:name=".ui.expiry.ExpiryActivity"
            android:exported="false" />
        <activity
            android:name=".ui.order.previeworder.PreViewOrder"
            android:exported="false" />
        <activity
            android:name=".ui.order.OrderActivity"
            android:exported="false" />
        <activity
            android:name=".ui.cart.CartActivity"
            android:exported="false" />
        <activity
            android:name=".ui.details.DetailsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.login.LoginActvity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.cart.search.SearchActivity"
            android:exported="false" />
        <activity
            android:name=".ui.cam.CamActivity"
            android:exported="false" />
        <activity
            android:name=".ui.main.MainActivity"
            android:exported="true" />
    </application>

</manifest>